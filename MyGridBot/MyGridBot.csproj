<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <OutputType>Exe</OutputType>
    <TargetFramework>net6.0</TargetFramework>
    <ImplicitUsings>enable</ImplicitUsings>
    <Nullable>enable</Nullable>
    <ApplicationIcon>LogBoVi.ico</ApplicationIcon>
    <Authors>$(BoVi)</Authors>
    <Copyright>BoVi</Copyright>
    <AssemblyName>$(MSBuildProjectName)</AssemblyName>
  </PropertyGroup>

  <ItemGroup>
    <Content Include="LogBoVi.ico" />
  </ItemGroup>

  <ItemGroup>
    <PackageReference Include="Bybit.Net" Version="3.14.1" />
    <PackageReference Include="ClosedXML.Report" Version="0.2.3" />
    <PackageReference Include="HtmlAgilityPack" Version="1.11.70" />
    <PackageReference Include="JK.Mexc.Net" Version="1.8.0" />
    <PackageReference Include="Telegram.Bot" Version="18.0.0" />
  </ItemGroup>

</Project>
